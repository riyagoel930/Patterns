#include<stdio.h>
int main(){
    int m,n;
    printf("Enter the no. of rows: ");
    scanf("%d",&n);
    
  for(int i=1;i<=n;i++)
  {
    m = i;
    for(int j=1;j<=i;j++)
    {
      
      m += n-j;
      printf("%d ", m);
    }
    printf("\n");
  }
  return 0;
}


OUTPUT:
Enter the no. of rows: 5
5 
6 9 
7 10 12 
8 11 13 14 
9 12 14 15 15 
